{
    "name": "jasco-b/query-filter",
    "description": "query filter for laravel",
    "type": "package",
    "license": "Mit",
    "authors": [
        {
            "name": "Jasurbek",
            "email": "bjasurbek94@gmail.com"
        }
    ],
    "minimum-stability": "dev",
    "require": {
        "php": "^7.1",
        "illuminate/database": "~5.6.34|~5.7.0|~5.8.0|^6.0|^7.0",
        "illuminate/http": "~5.6.34|~5.7.0|~5.8.0|^6.0|^7.0",
        "illuminate/support": "~5.6.34|~5.7.0|~5.8.0|^6.0|^7.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^7.0|^8.0|^9.0",
        "orchestra/testbench": "~3.6.0|~3.7.0|~3.8.0|^4.0|^5.0",

        "ext-json": "*"
    },
    "autoload-dev": {
        "psr-4": {
            "JascoB\\QueryFilter\\Tests\\": "tests"
        }
    },
    "autoload": {
        "psr-4": {
            "JascoB\\QueryFilter\\": "src"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "phpunit --coverage-html coverage"
    },
    "config": {
        "sort-packages": true
    },
    "prefer-stable": true
}
